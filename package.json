{
  "name": "teste-nemu-back",
  "version": "1.0.0",
  "description": "Backend API for processing user journey data from XLSX files",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.json && tsc --project tsconfig.functions.json",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "journey-processing"
  ],
  "author": "Izadora Cury Pierette",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "serverless-http": "^3.2.0",
    "typescript": "^5.3.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0"
  }
}
